  echo $ECHO_N "(cached) $ECHO_C" >&6
else

	cf_cv_DEFAULT_PAGER="$DEFAULT_PAGER"
	if test -z "$cf_cv_DEFAULT_PAGER"; then
		set -- $PAGER more less most pg;
		while test $# != 0; do
			cf_word=$1${PROG_EXT}
			case $1 in
			-*)
				;;
			*)
				if test -f "$cf_word" && test ! -f "./$cf_word" && test -x "$cf_word"; then
					cf_cv_DEFAULT_PAGER="$cf_word"
				else
